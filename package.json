{
  "name": "html-inject-commands",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.15.2",
  "description": "Inject commands result into index.html meta tag.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byronogis/vite-plugin-html-inject-commands.git"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "automd": "turbo run automd && pnpm run root:automd",
    "root:automd": "automd README.md",
    "release": "bumpp -r && pnpm run -r release",
    "typecheck": "turbo run typecheck",
    "root:lint": "eslint .",
    "root:lint:fix": "eslint . --fix",
    "clear": "npx turbo run clear && rimraf **/node_modules **/.turbo",
    "prepare": "simple-git-hooks",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "automd": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "jiti": "catalog:",
    "lint-staged": "catalog:",
    "rimraf": "catalog:",
    "simple-git-hooks": "catalog:",
    "turbo": "latest",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
